{
    "name": "iris",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "clean:svelte": "rimraf .svelte-kit",
        "frontend": "vite dev --port 5173",
        "dev": "npm run frontend -- --host 0.0.0.0",
        "build": "vite build",
        "preview": "vite preview",
        "dev-with-backend": "run-p 'backend' 'dev'",
        "backend": "python ../enhanced_launcher.py",
        "sync": "svelte-kit sync",
        "check": "svelte-check",
        "start:prod": "node build/index.js",
        "validate:assets": "node tools/assets/validate-manifest.mjs assets/3d/luxury/ASSET_MANIFEST.json --maxTris=100000 --maxTexDim=4096 --maxTexBytes=16777216 --maxAlphaDim=2048 --maxAlphaBytes=8388608 --maxSetBytes=52428800"
    },
    "devDependencies": {
        "@playwright/test": "^1.28.1",
        "@sveltejs/adapter-auto": "^2.0.0",
        "@sveltejs/adapter-node": "^1.3.1",
        "@sveltejs/kit": "1.30.4",
        "@sveltejs/vite-plugin-svelte": "2.5.3",
        "@types/d3": "^7.4.3",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "@webgpu/types": "^0.1.64",
        "autoprefixer": "^10.4.21",
        "eslint": "^8.28.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-svelte": "^2.30.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.8.0",
        "prettier-plugin-svelte": "^2.10.1",
        "rimraf": "^5.0.0",
        "svelte": "4.2.20",
        "svelte-check": "^3",
        "tslib": "^2.4.1",
        "typescript": "~5.4",
        "vite": "4.5.14"
    },
    "type": "module",
    "dependencies": {
        "@auth/core": "^0.18.6",
        "@auth/sveltekit": "^0.3.16",
        "@gltf-transform/functions": "^4.2.1",
        "@tailwindcss/postcss": "^4.1.11",
        "d3": "^7.9.0",
        "isomorphic-dompurify": "^2.26.0",
        "mathjs": "^14.6.0",
        "tailwindcss": "^4.1.11"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Jasonleonardvolk/iRis.git"
    },
    "homepage": "https://github.com/Jasonleonardvolk/iRis#readme",
    "bugs": {
        "url": "https://github.com/Jasonleonardvolk/iRis/issues"
    },
    "description": "iRis - Holographic UI with memory synthesis",
    "keywords": [
        "iris",
        "holographic",
        "ui",
        "svelte",
        "sveltekit",
        "webgpu",
        "memory-synthesis"
    ],
    "author": "Jason Leonard Volk",
    "license": "MIT"
}
